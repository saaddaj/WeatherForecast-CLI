{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "MusementApi": {
    "BaseAddress": "https://api.musement.com",
    "HandlerLifeTime": "1.00:00:00" // 1 day
  },

  "WeatherApi": {
    "BaseAddress": "http://api.weatherapi.com",
    "HandlerLifeTime": "0.00:05:00", // 5 minutes
    "ApiKey": "$(ApiKey)"
  }
}
